{
    "name": "view-launcher/blade-view-launcher",
    "description": "A Laravel package for opening blade view/component in code editor from browsers.",
    "keywords": [
        "dev",
        "debug",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "scripts": {
        "fix-cs": ["prettier 'src/**/*.php' --write"]
    },
    "require": {
        "yaquawa/look-alike": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ViewLauncher\\Blade\\ViewLauncherServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "ViewLauncher\\Blade\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
